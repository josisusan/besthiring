ProfileC#new
<%= simple_form_for [@user, @profile],:url => user_profile_path(@user) do |f| %>
<%= f.input :name %>
<%= f.input :phone, :as => :tel %>
<%#= f.input :email, :as => :email %>
<%= f.input :dob, :as => :date, :start_year => Date.today.year - 50, :end_year => Date.today.year - 12 %>
<%= f.input :academic %>
<%= f.input :experience %>
<%= f.submit %>
<% end %>